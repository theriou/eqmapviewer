<Window x:Class="eqmaps.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eqmaps"
        mc:Ignorable="d"
        Title="EQ Map Viewer" Height="720" Width="1280">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Open" Click="MenuItem_Open"/>
            <MenuItem Header="_Refresh" Click="MenuItem_Refresh"/>
        </Menu>
        <Canvas Name="myCanvas">
            <Canvas.Background>
                <ImageBrush ImageSource="/images/cart.png" Stretch="UniformToFill"/>
            </Canvas.Background>
        </Canvas>
    </DockPanel>
</Window>
